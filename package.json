{
  "name": "unityapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "pod-reinstall": "rm -rf ios/Pods ios/Podfile.lock && npx pod-install",
    "clean:modules": "rm -rf node_modules package-lock.json && npm cache clean --force",
    "reset:metro": "watchman watch-del-all || true && rm -rf $TMPDIR/metro-* $TMPDIR/react-* && rm -rf /tmp/metro-* /tmp/haste-map-* || true",
    "clean:ios": "rm -rf ios/build && xcodebuild -workspace ios/unityapp.xcworkspace -scheme unityapp -configuration Debug clean || true && rm -rf ~/Library/Developer/Xcode/DerivedData/*",
    "clean": "npm run clean:modules && npm install && npm run pod-reinstall && npm run reset:metro",
    "postinstall": "patch-package"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@azesmway/react-native-unity": "^1.0.11",
    "@miblanchard/react-native-slider": "^2.6.0",
    "@notifee/react-native": "^9.1.8",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-firebase/app": "23.4.0",
    "@react-native-firebase/in-app-messaging": "23.4.0",
    "@react-native-firebase/messaging": "23.4.0",
    "@react-native-google-signin/google-signin": "^16.0.0",
    "@react-navigation/bottom-tabs": "^6.6.0",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@reduxjs/toolkit": "^2.2.6",
    "@stripe/stripe-react-native": "^0.37.3",
    "axios": "^1.7.7",
    "lucide-react-native": "^0.468.0",
    "react": "^18.2.0",
    "react-native": "^0.73.6",
    "react-native-dotenv": "^3.4.8",
    "react-native-hyperkyc-sdk": "^0.17.0",
    "react-native-image-picker": "^8.2.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-pager-view": "^6.9.1",
    "react-native-razorpay": "^2.3.0",
    "react-native-safe-area-context": "^4.10.0",
    "react-native-screens": "3.37.0",
    "react-native-svg": "13.9.0",
    "react-native-tab-view": "^4.1.3",
    "react-native-webview": "^13.16.0",
    "react-redux": "^9.1.2",
    "supertokens-react-native": "^5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/cli": "^14.0.0",
    "@react-native-community/cli-platform-ios": "^14.0.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "18.2.6",
    "@types/react-test-renderer": "18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "patch-package": "^8.0.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  }
}
